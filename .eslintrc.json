{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/jsx-max-props-per-line": 1,
    "react/jsx-first-prop-new-line": 2,
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "react/jsx-closing-bracket-location": 1,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ]
  }
}